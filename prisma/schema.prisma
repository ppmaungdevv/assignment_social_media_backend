generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model User {
  id       Int     @id @default(autoincrement())
  name     String
  email    String? @unique
  password String
}

model RevokedToken {
  id       Int      @id @default(autoincrement())
  token    String
  expireAt DateTime
}
